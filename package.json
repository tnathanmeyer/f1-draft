  "eslintIgnore": [
    "dist/*"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-fetch": "^3.0.0",
    "csso": "^5.0.3",
    "eslint": "^8.11.0",
    "html-minifier": "^4.0.0",
    "sass": "^1.49.9"
  }
}
