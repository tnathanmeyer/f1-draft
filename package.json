{
  "private": true,
  "name": "f1-draft",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "build:gh": "GITHUB_PAGES=f1-draft preact build && dot-json ./build/manifest.json start_url \"/f1-draft/\"",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "preact"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "dot-json": "^1.2.2",
    "eslint": "^8.14.0",
    "eslint-config-preact": "^1.3.0",
    "preact-cli": "^3.3.5",
    "sass": "^1.50.1",
    "sass-loader": "^10.2.1",
    "sirv-cli": "^2.0.2"
  },
  "dependencies": {
    "preact": "^10.7.1",
    "preact-render-to-string": "^5.1.21"
  }
}
